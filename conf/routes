# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()


# Home page
GET     /searchFirstSize            controllers.JavaString.searchFirstSize()
GET     /searchLastSize             controllers.JavaString.searchLastSize()


GET     /createArray                controllers.JavaArray.createArray()
GET     /printArray                 controllers.JavaArray.printArray()



# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
